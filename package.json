{
  "name": "aq-monorepo",
  "private": true,
  "version": "0.0.0",
  "packageManager": "bun@1.2.22",
  "engines": {
    "bun": ">=1.2.22",
    "node": ">=18"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "bun -w run build",
    "test": "bun -w run test",
